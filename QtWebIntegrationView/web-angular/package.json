{
  "name": "webhost-angular-sample",
  "version": "0.1.0",
  "private": true,
  "description": "Sample Angular app for Qt WebHost integration",
  "scripts": {
    "start": "ng serve --configuration development",
    "build": "ng build --configuration development --output-path ../web --base-href ./",
    "build:prod": "ng build --configuration production --output-path ../web --base-href ./",
    "watch": "ng build --watch --configuration development --output-path ../web --base-href ./",
    "test": "ng test --watch=false",
    "sync:hostapi": "node scripts/sync-hostapi.js"
  },
  "dependencies": {
    "@angular/animations": "^17.2.0",
    "@angular/cdk": "^17.2.0",
    "@angular/common": "^17.2.0",
    "@angular/compiler": "^17.2.0",
    "@angular/core": "^17.2.0",
    "@angular/forms": "^17.2.0",
    "@angular/material": "^17.2.0",
    "@angular/platform-browser": "^17.2.0",
    "@angular/platform-browser-dynamic": "^17.2.0",
    "@angular/router": "^17.2.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.3",
    "@qt-web/hostapi": "file:../build/generated/hostapi/npm/qt-web-hostapi-0.1.0.tgz"
  },
  "devDependencies": {
    "@angular/cli": "^17.2.0",
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/compiler-cli": "^17.2.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.11.19",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "^5.3.3"
  }
}
